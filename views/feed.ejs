
<% include ./partials/header.ejs %>


<div class="w-full min-h-screen bg-white px-10 py-8 text-black">
    <div class="cards  flex flex-wrap gap-10">
        <% posts.forEach(function(elem){ %>
            <div class=" card w-52">
                <div class=" w-full min-h-[100px] rounded-lg overflow-hidden">
                    <img  src="/images/uploads/<%=elem.image %>" alt="">
                </div>
                <h5 class="mt-2 text-sm" ><%= elem.title %></h5>
    
            </div>
       <% }) %>
        
       
    </div>
</div>

<% include ./partials/footer.ejs %>