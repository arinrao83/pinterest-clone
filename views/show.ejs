<% include ./partials/header.ejs %>

<div class="w-full min-h-screen bg-white px-10 py-8 text-black">
    <div class="cards  flex flex-wrap gap-10">
        <% user.posts.forEach(function(elem){ %>
            <div class=" card w-[250px] h-[300px]">
                <div class=" w-full h-full  rounded-lg overflow-hidden">
                    <img class="object-cover "  src="/images/uploads/<%=elem.image %>" alt="">
                </div>
                <h5 class="mt-2 text-xl" ><%= elem.title %></h5>
    
            </div>
       <% }) %>
      
       
    </div>
</div>

<% include ./partials/footer.ejs %>